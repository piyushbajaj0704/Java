class Solution {
    public double findMaxAverage(int[] nums, int k) {
        double max,sum=0,avg;
        for(int i=0;i<k;i++) {
            sum += nums[i];
        }
        max = sum/k;  
        int j=0;
        for(int i=k;i<nums.length;i++) {
            sum += nums[i] - nums[j++];
            avg = sum/k;
            max = Math.max(max,avg);
        }
        return max;
    }
}
